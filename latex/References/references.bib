issn = {1040-4651},
journal = {The Plant cell},
keywords = {Alleles,Amino Acid Sequence,Arabidopsis,Arabidopsis: genetics,Arabidopsis: growth \& development,Base Sequence,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Plant,DNA, Plant: genetics,Enhancer Elements, Genetic,Gene Expression Regulation, Developmental,Gene Expression Regulation, Plant,Genes, Plant,Light,MicroRNAs,MicroRNAs: genetics,Models, Genetic,Mutagenesis, Insertional,Plants, Genetically Modified,Promoter Regions, Genetic,RNA, Plant,RNA, Plant: genetics,Sequence Homology, Nucleic Acid,Temperature},
month = mar,
number = {3},
pages = {705--21},
pmid = {15722475},
