journal = {The Plant journal : for cell and molecular biology},
keywords = {Base Sequence,Bryophyta,Bryophyta: genetics,Bryophyta: metabolism,Chromosomes, Plant,Cloning, Molecular,Contig Mapping,Expressed Sequence Tags,Gene Expression Regulation, Plant,Gene Expression Regulation, Plant: physiology,Germ Cells,Germ Cells: physiology,MicroRNAs,MicroRNAs: chemistry,MicroRNAs: physiology,Naphthaleneacetic Acids,Naphthaleneacetic Acids: metabolism,Up-Regulation},
month = sep,
number = {6},
pages = {837--48},
pmid = {16146523},
title = {{Cloning and characterization of micro-RNAs from moss.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16146523},
