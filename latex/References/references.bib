    month = dec,
    number = {3},
    pages = {283--91},
    pmid = {24018204},
    publisher = {Elsevier Inc.},
    title = {{Construction of Specific Parallel Amplification of RNA Ends (SPARE) libraries for the systematic identification of plant microRNA processing intermediates.}},
