@article{Mateos2010,
    abstract = {MicroRNAs (miRNAs) are widespread posttranscriptional regulators of gene expression. They are processed from longer primary transcripts that contain foldback structures (reviewed in). In animals, a complex formed by Drosha and DGCR8/Pasha recognizes the transition between the single-stranded RNA sequences and the stem loop to produce the first cleavage step in miRNA biogenesis. Whereas animal precursors are of uniform size and shape, their plant counterparts comprise a collection of variable stem loops, and little is known about the structural clues recognized during their processing. Here, we designed an unbiased approach based on the random mutagenesis of the MIR172a precursor to study miRNA processing in plants. Randomly mutated precursors were overexpressed in Arabidopsis, and their activity was determined in vivo. We gathered sequence data from these transgenes and used it to build a MIR172a precursor map highlighting relevant and neutral positions for its processing. A 15 nucleotide stem segment below the miRNA/miRNA() duplex was essential for MIR172a processing. In contrast, mutations in the terminal-loop region were mostly neutral, yet a loop was required for miR172 biogenesis. The results could be extended to other precursors, suggesting the existence of common features in at least part of the plant precursors.},
    author = {Mateos, Julieta L and Bologna, Nicol\'{a}s G and Chorostecki, Uciel and Palatnik, Javier F},
    institution = {Instituto de Biolog\'{\i}a Molecular y Celular de Rosario, Suipacha 531, 2000 Rosario, Argentina.},
    journal = {Current Biology},
    keywords = {arabidopsis,arabidopsis genetics,arabidopsis metabolism,base sequence,genetically modified,micrornas,micrornas chemistry,micrornas genetics,micrornas metabolism,molecular sequence data,mutagenesis,nucleic acid conformation,plant,plant chemistry,plant genetics,plant metabolism,plants,post transcriptional,rna,rna precursors,rna precursors chemistry,rna precursors genetics,rna precursors metabolism,rna processing},
    number = {1},
    pages = {49--54},
    pmid = {20005105},
    title = {{Identification of microRNA processing determinants by random mutagenesis of Arabidopsis MIR172a precursor.}},
    url = {http://www.ncbi.nlm.nih.gov/pubmed/20005105},
    volume = {20},
    year = {2010}
}

